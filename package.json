{
  "name": "meridiana-child-theme",
  "version": "1.0.0",
  "description": "Child theme per piattaforma formazione Cooperativa La Meridiana",
  "scripts": {
    "watch": "npm-run-all --parallel watch:*",
    "watch:scss": "sass --watch assets/css/src:assets/css/dist --style expanded --source-map",
    "watch:js": "webpack --watch --mode development",
    "build": "npm-run-all build:*",
    "build:scss": "sass assets/css/src:assets/css/dist --style compressed --no-source-map",
    "build:js": "webpack --mode production",
    "dev": "npm run watch"
  },
  "keywords": [
    "wordpress",
    "child-theme",
    "blocksy"
  ],
  "author": "La Meridiana",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "chart.js": "^4.4.1"
  },
  "devDependencies": {
    "alpinejs": "^3.13.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dompurify": "^3.3.0"
  }
}
